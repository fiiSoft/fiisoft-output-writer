{
    "name": "fiisoft/output-writer",
    "type": "library",
    "description": "Abstraction of a writer (of messages) to an output (console, log, stream etc).",
    "keywords": ["console","writer","output"],
    "homepage": "https://github.com/fiiSoft/fiisoft-output-writer",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "FiiSoft",
            "email": "fiisoft.production@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "FiiSoft\\Test\\": "tests/",
            "FiiSoft\\": "src/"
        }
    },
    "require": {
        "php": ">=5.6.0",
        "roave/security-advisories": "dev-master"
    },
    "suggest": {
        "symfony/console": "Required to use SymfonyConsoleOutputWriter as a wrapper to OutputInterface from Symfony"
    }
}
